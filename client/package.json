{
  "name": "nomad-house-client",
  "version": "0.0.1",
  "author": "Matthew Keil",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "build-report": "vue-cli-service build --modern --report",
    "inspect-bundle": "vue-cli-service inspect",
    "inspect-bundle-prod": "vue-cli-service inspect --mode=production",
    "lint-ts": "eslint --no-error-on-unmatched-pattern --fix --ext .js,.ts,.vue",
    "lint-ts-src": "npm run lint-ts -- src/",
    "lint-ts-all": "npm run lint-ts -- src/ tests/ public/",
    "lint-style": "stylelint --fix",
    "lint-style-src": "npm run lint-style -- \"src/**/*.{vue,scss}\"",
    "lint-style-all": "npm run lint-style -- \"{src,tests,public}/**/*.{vue,scss}\"",
    "lint-md": "markdownlint --fix",
    "lint-md-src": "npm run lint-md -- \"src/*.md\"",
    "lint-md-all": "npm run lint-md -- \"public/**/*.md\" \"src/**/*.md\" \"tests/**/*.md\"",
    "lint-prettier": "prettier --write --loglevel warn",
    "lint-prettier-src": "npm run lint-prettier -- \"src/**/*.{ts,js,json,css,scss,vue,html,md}\"",
    "lint-prettier-all": "npm run lint-prettier -- \"**/*.{ts,js,json,css,scss,vue,html,md}\"",
    "lint-all": "run-p lint-*-all",
    "lint": "run-p lint-*-src",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "gitHooks": {
    "pre-push": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "markdownlint-cli": "^0.22.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.1",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "_id": "nomad-house-client@0.0.1"
}
