{
  "name": "client",
  "version": "1.0.0",
  "description": "My cat&#39;s meow Nuxt.js project",
  "author": "Matthew Keil",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
